    "def lcm(x,y):\n",
    "    \"\"\"Function to find LCM of Two numbers\"\"\"\n",
    "    greater = x if x>y else y\n",
    "    \n",
    "    while True:\n",
    "        if(greater%x==0) and (greater%y==0):\n",
    "            break\n",
    "        else:\n",
    "            greater += 1\n",
    "    print(f'LCM of {x} and {y} is {greater}')\n",
    "    \n",
    "    \n",
    "lcm(12,25)"
