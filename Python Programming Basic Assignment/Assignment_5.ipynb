    "def hcf(x,y):\n",
    "    \"\"\"Function to find HCF of Two numbers\"\"\"\n",
    "    low = x if x<y else y\n",
    "    \n",
    "    for i in range(1, low+1):\n",
    "        if(x%i==0) and (y%i==0):\n",
    "            hcf=i\n",
    "    print(f'HCF of {x} and {y} is {hcf}')\n",
    "    \n",
    "    \n",
    "hcf(12,18)"
   ]
